{
  "manifest_version": 3,
  "name": "Image Grabber",
  "description": "Extract all images from current web page",
  "version": "1.0",
  "author": "Brytvina Alona",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
  ],
  "background":{}
}
